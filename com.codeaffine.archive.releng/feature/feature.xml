<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.codeaffine.archive.feature"
      label="Archive Utilities Feature"
      version="0.1.0.qualifier"
      provider-name="Codeaffine.com"
      plugin="com.codeaffine.archive.ui">

   <description url="http://rherrmann.github.com/archive/index.html">
      This feature is an extensions to the Eclipse IDE to show the
structure and content and to extract archive files (zip, jar,
war, etc.) within the workspace.
   </description>

